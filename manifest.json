{
  "manifest_version": 3,
  "name": "Auto-Pinning Extension",
  "version": "1.0",
  "description": "Automatically pin tabs based on rules and show them in the extension popup.",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
